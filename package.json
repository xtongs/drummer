{
  "name": "drummer",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky",
    "generate-icons": "node scripts/generate-icons.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "check:spec": "bun scripts/check-spec-coverage.mjs",
    "version:bump": "node scripts/bump-version.mjs",
    "version:bump:major": "node scripts/bump-version.mjs major",
    "version:bump:minor": "node scripts/bump-version.mjs minor",
    "version:bump:patch": "node scripts/bump-version.mjs patch",
    "version:check": "node scripts/check-version-bump.mjs"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vexflow": "^5.0.0"
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.9",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "happy-dom": "^20.3.1",
    "husky": "^9.1.7",
    "jsdom": "24.1.3",
    "typescript": "^5.2.2",
    "unicode-canonical-property-names-ecmascript": "^2.0.1",
    "vite": "^4.5.14",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.17",
    "workbox-core": "^7.4.0",
    "workbox-precaching": "^7.4.0"
  }
}
